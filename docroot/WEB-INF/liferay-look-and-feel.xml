<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 6.2.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_6_2_0.dtd">

<look-and-feel>
	<compatibility>
		<version>6.2.1+</version>
	</compatibility>
	<theme id="redefined" name="Redefined">
			<settings>
			<setting configurable="false" key="show-site-name-default" value="true" />
			<setting configurable="false" key="show-site-name-supported" value="true" />
			<setting configurable="true" key="secondary-color" options="blue-powder,blue-navy,crimson,gold,grey,green-dark,green-light,maroon,orange,purple,red,yellow" type="select" value="grey" />
			<setting configurable="true" key="site-headline" type="text"></setting>
			<setting configurable="true" key="tagline" type="text"></setting>
			<setting configurable="true" key="masthead-image" type="text"></setting>
			<setting configurable="true" key="background-image" type="text"></setting>
				<setting configurable="true" key="favicon" type="text"></setting>
			<setting configurable="true" key="address" type="text"></setting>
			<setting configurable="true" key="phone" type="text"></setting>
			<setting configurable="true" key="fax" type="text"></setting>
			<setting configurable="true" key="email" type="text"></setting>
			<setting configurable="true" key="facebook" type="text"></setting>
			<setting configurable="true" key="twitter" type="text"></setting>
			<setting configurable="true" key="youtube" type="text"></setting>
			<setting configurable="true" key="instagram" type="text"></setting>
		</settings>
				<roles>
			<role-name>User</role-name>
		</roles>
		<color-scheme id="01" name="Light">
		    <default-cs>true</default-cs>
			<css-class>light</css-class>
		</color-scheme>
		<color-scheme id="02" name="Dark">
			<css-class>dark</css-class>
		</color-scheme>
				<layout-templates>
			<custom>
				<layout-template id="1_2_1" name="1-2-1">
					<template-path>/layouttpl/custom/1_2_1.tpl</template-path>
					<wap-template-path>/layouttpl/custom/1_2_1.wap.tpl</wap-template-path>
					<thumbnail-path>/layouttpl/custom/custom.png</thumbnail-path>
				</layout-template>
				<layout-template id="0_2_1" name="0-2-1">
					<template-path>/layouttpl/custom/0_2_1.tpl</template-path>
					<wap-template-path>/layouttpl/custom/0_2_1.wap.tpl</wap-template-path>
					<thumbnail-path>/layouttpl/custom/custom.png</thumbnail-path>
				</layout-template>
								<layout-template id="1_column" name="1-Column">
					<template-path>/layouttpl/custom/1_column.tpl</template-path>
					<wap-template-path>/layouttpl/custom/1_column.wap.tpl</wap-template-path>
					<thumbnail-path>/layouttpl/custom/custom.png</thumbnail-path>
				</layout-template>
								<layout-template id="2_sidebar_right" name="Sidebar Right">
					<template-path>/layouttpl/custom/2_sidebar_right.tpl</template-path>
					<wap-template-path>/layouttpl/custom/2_sidebar_right.wap.tpl</wap-template-path>
					<thumbnail-path>/layouttpl/custom/custom.png</thumbnail-path>
				</layout-template>
				<layout-template id="2_sidebar_left" name="Sidebar Left">
					<template-path>/layouttpl/custom/2_sidebar_left.tpl</template-path>
					<wap-template-path>/layouttpl/custom/2_sidebar_left.wap.tpl</wap-template-path>
					<thumbnail-path>/layouttpl/custom/custom.png</thumbnail-path>
				</layout-template>
			</custom>
		</layout-templates>
		</theme>
</look-and-feel>